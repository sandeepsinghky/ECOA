
<%@ include file="../../../include/taglib.jsp"%>

<div id="content">
	<div class="gutter">

    </br>
    </br>

		<table width="80%" style="align: left;" border="0">

       <c:choose>
			<c:when test="${isLogOut}">
				<tr>
					<td colspan="6" style="font-weight: bold; text-align: center;">Your login was successful; however, a technical issue was detected and your login has been temporarily invalidated. An Administrator has been notified of the error description.  Please try to login again later.</td>

				</tr>
				
			</c:when>
			<c:otherwise>
				<tr>
					<td colspan="6" style="font-weight: bold; text-align: center;">We are having technical problems. Please try in a few minutes. The Technical Team has been notified. </td>

				</tr>
				
				
			</c:otherwise>
		</c:choose>
		
         </table>


	</div>
</div>

